                li(class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref)
                    a(class="text-sm font-bold text-neutral-500 transition duration-200 hover:text-neutral-700 hover:ease-in-out focus:text-neutral-700 disabled:text-black/30 motion-reduce:transition-none dark:text-neutral-200 dark:hover:text-neutral-300 dark:focus:text-neutral-300 lg:px-2 [&.active]:text-black/90 dark:[&.active]:text-zinc-400" href="/promociones" data-te-nav-link-ref) Promociones
                li(class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref)
                    a(class="text-sm font-bold text-neutral-500 transition duration-200 hover:text-neutral-700 hover:ease-in-out focus:text-neutral-700 disabled:text-black/30 motion-reduce:transition-none dark:text-neutral-200 dark:hover:text-neutral-300 dark:focus:text-neutral-300 lg:px-2 [&.active]:text-black/90 dark:[&.active]:text-zinc-400" href="/laptops" data-te-nav-link-ref) Computadoras 
                li(class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref)
                    a(class="text-sm font-bold text-neutral-500 transition duration-200 hover:text-neutral-700 hover:ease-in-out focus:text-neutral-700 disabled:text-black/30 motion-reduce:transition-none dark:text-neutral-200 dark:hover:text-neutral-300 dark:focus:text-neutral-300 lg:px-2 [&.active]:text-black/90 dark:[&.active]:text-zinc-400" href="/telefonos" data-te-nav-link-ref) Celulares 
                li(class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref)
                    a(class="text-sm font-bold text-neutral-500 transition duration-200 hover:text-neutral-700 hover:ease-in-out focus:text-neutral-700 disabled:text-black/30 motion-reduce:transition-none dark:text-neutral-200 dark:hover:text-neutral-300 dark:focus:text-neutral-300 lg:px-2 [&.active]:text-black/90 dark:[&.active]:text-zinc-400" href="/tablets" data-te-nav-link-ref) Tablets
                li(class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref)
                    form(action="/buscador" method="POST" class="flex lg:flex-row gap-3 items-baseline")
                        input(type="hidden" name="_csrf" value= csrfToken)
                        label(for="termino" class="text-sm uppercase font-bold text-white") Busqueda  
                        input(type="text" id="termino" name="termino"  placeholder="Buscar Productos" class="p-1 shadow text-sm")
                        input(type="submit" value="Buscar" class="bg-indigo-800 rounded-lg hover:bg-indigo-400 text-white uppercase font-bold p-1 cursor-pointer text-sm")
        div(class="relative flex items-center"  data-te-dropdown-ref data-te-dropdown-alignment="end")
            if token
                a(class="relative flex items-center lg:pr-2 text-sm font-bold text-white" href='/auth/login') Iniciar Sesion
                a(class="relative flex items-center lg:pr-2 text-sm font-bold text-white" href='/auth/registro') Crear Cuenta  
            else
                form(action="/auth/cerrar-sesion" method="POST")
                    input(type="hidden" name="_csrf" value=csrfToken) 
                    input(type="submit" class="text-sm font-bold relative flex items-center text-white lg:pr-2 cursor-pointer" value="Cerrar Sesion")         

